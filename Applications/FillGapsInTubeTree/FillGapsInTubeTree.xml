<?xml version="1.0" encoding="UTF-8"?>
<executable>
  <category>TubeTK</category>
  <title>Fill Gaps In Tube Tree</title>
  <description>Interpolate the gap between parent and child in a tube-tree.</description>
  <version>1.0</version>
  <documentation-url>http://public.kitware.com/Wiki/TubeTK</documentation-url>
  <license>Apache 2.0</license>
  <contributor>Sumedha Singla (Kitware)</contributor>
  <acknowledgements>This work is part of the TubeTK project at Kitware.</acknowledgements>
  <parameters>
    <label>IO</label>
    <description>Input/output parameters.</description>
    <file>
      <name>inputTree</name>
      <label>Input Tree</label>
      <channel>input</channel>
      <index>0</index>
      <description>Input Tube Tree</description>
    </file>
    <file>
      <name>outputTree</name>
      <label>Output Tree</label>
      <channel>output</channel>
      <index>1</index>
      <description>Output Tube Tree</description>
    </file>
    <string-enumeration>
      <name>InterpolationMethod</name>
      <label>Method</label>
      <description>Interpolation method to extract path</description>
      <longflag>method</longflag>
      <default>Straight_Line</default>
      <element>Linear_Interpolation</element>
      <element>Curve_Fitting</element>
      <element>Minimal_Path</element>
    </string-enumeration>
  </parameters>
</executable>
