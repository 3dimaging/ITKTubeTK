<?xml version="1.0" encoding="UTF-8"?>
<executable>
  <category>TubeTK</category>
  <title>Sub-sample Tubes (TubeTK)</title>
  <description>Sub-samples the points comprising a tube tree.</description>
  <version>0.1.0.$Revision: 2104 $(alpha)</version>
  <documentation-url>http://public.kitware.com/Wiki/TubeTK</documentation-url>
  <license>Apache 2.0</license>
  <contributor>Matthew M. McCormick (Kitware)</contributor>
  <acknowledgements>This work is part of the TubeTK project at Kitware.</acknowledgements>
  <parameters>
    <label>IO</label>
    <description>Input/output parameters.</description>
    <file>
      <name>inputTubeFile</name>
      <label>Input Tube File</label>
      <description>Input tubes.</description>
    </file>
    <integer>
      <name>samplingFactor</name>
      <flag>s</flag>
      <longflag>samplingFactor</longflag>
      <label>Sampling Factor</label>
      <default>1</default>
      <description>Sub-sampling decimation factor.</description>
    </integer>
    <file>
      <name>outputTubeFile</name>
      <label>Output Tube File</label>
      <description>Output tubes.</description>
    </file>
  </parameters>
</executable>
